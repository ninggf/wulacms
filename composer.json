{
    "name": "wula/wulacms",
    "description": "a content manager system powered by wulaphp.",
    "require": {
        "php": ">= 7.2",
        "ext-json": "*",
        "ext-curl": "*",
        "ext-mbstring": "*",
        "ext-gd": "*",
        "ext-pcre": "*",
        "ext-session": "*",
        "ext-mysqlnd": "*",
        "ext-pdo": "*",
        "ext-pdo_mysql": "*",
        "ext-SPL": "*",
        "ext-Reflection": "*",
        "wula/wulaphp": "^3.9",
        "wulacms/system": "^3.0",
        "wulacms/backend": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5"
    },
    "suggest": {
        "ext-yac": "*"
    },
    "license": "MIT",
    "keywords": [
        "CMF",
        "CMS",
        "wulaphp",
        "php",
        "adminui"
    ],
    "time": "2018-06-20",
    "type": "project",
    "config": {
        "preferred-install": "dist",
        "optimize-autoloader": true,
        "classmap-authoritative": true
    },
    "authors": [
        {
            "name": "Leo Ning",
            "email": "windywany@gmail.com"
        }
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "3.0-dev"
        }
    },
    "support": {
        "issues": "https://github.com/ninggf/wulacms/issues"
    },
    "homepages": "https://www.wulacms.com/"
}
