{
    "name": "wula/wulacms",
    "description": "我们努力做一个比较优秀的内容管理框架(CMF)，争取让她像异星战场中的乌拉一样快",
    "require": {
        "php": "~7.2",
        "ext-json": "*",
        "ext-PDO": "*",
        "ext-pdo_mysql": "*",
        "ext-mbstring": "*",
        "ext-curl": "*",
        "ext-gd": "*",
        "ext-SPL": "*",
        "ext-pcre": "*",
        "ext-session": "*",
        "ext-mysqlnd": "*",
        "ext-Reflection": "*",
        "wula/wulaphp": "~3.7.0",
        "wula/cms-support": "^3.0",
        "wula/jqadmin": "^2.4",
        "wulacms/backend": "^2.4",
        "wulacms/system": "^2.4",
        "wulacms/media": "^2.4",
        "wulacms/rest": "^2.4",
        "wulacms/cms": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5"
    },
    "suggest": {
        "ext-posix": "*",
        "ext-pcntl": "*",
        "ext-sockets": "*",
        "ext-SimpleXML": "*",
        "ext-gearman": "*",
        "ext-redis": "*",
        "ext-apcu": "*",
        "ext-yac": "*",
        "ext-memcached": "*"
    },
    "license": "MIT",
    "keywords": [
        "CMF",
        "CMS",
        "wulaphp"
    ],
    "time": "2018-06-20",
    "type": "project",
    "config": {
        "preferred-install": "dist",
        "optimize-autoloader": true,
        "classmap-authoritative": true,
        "allow-plugins": {
            "wula/wula-installer": true
        }
    },
    "authors": [
        {
            "name": "Leo Ning",
            "email": "windywany@gmail.com"
        }
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        }
    },
    "support": {
        "issues": "https://github.com/ninggf/wulacms/issues"
    },
    "homepages": "http://www.wulacms.com/"
}
