  "require": {
	"php": ">=5.6 || >= 7.1",
	"ext-json": "*",
	"ext-pdo": "*",
	"ext-pdo_mysql": "*",
	"ext-mbstring": "*",
	"ext-curl": "*",
	"ext-SPL": "*",
	"ext-mysqlnd": "*",
	"wula/wulaphp": "*",
	"wula/cms-support": "*",
	"wula/ui": "*",
	"wula/form": "*",
	"wulacms/core": "*",
	"wulacms/dashboard": "*"
  },
  "require-dev": {
	"psr/log": "^1.0",
	"phpunit/phpunit": "*",
	"ralouphie/mimey": "^1.0"
  },
  "license": "MIT",
  "keywords": [
	"CMF",
	"CMS"
  ],
  "time": "2016-12-07",
  "scripts": {
	"post-create-project-cmd": [
	  "php artisan init"
	]
  },
  "type": "project",
  "config": {
	"preferred-install": "dist"
  },
  "authors": [
	{
	  "name": "Leo Ning",
