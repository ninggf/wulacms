  "require": {
	"php": ">=5.6 || >= 7.1",
	"ext-json": "*",
	"ext-pdo": "*",
	"ext-pdo_mysql": "*",
	"ext-mbstring": "*",
	"ext-xml": "*",
	"ext-curl": "*",
	"ext-SPL": "*",
	"ext-mysqlnd": "*",
	"michelf/php-markdown": "^1.7",
	"imagine/imagine": "^0.7.1",
	"wula/wulaphp": "^1.9.0",
	"wula/curlient": "^1.0.0",
	"wula/cms-support": "^1.0.0",
	"wula/jqadmin": "^1.3.0",
	"wulacms/backend": "^1.0.0",
	"wulacms/system": "^1.0.0"
  },
  "license": "MIT",
  "keywords": [
	"CMF",
	"CMS"
  ],
  "time": "2016-12-07",
  "scripts": {
	"post-create-project-cmd": [
	  "php artisan init"
	]
  },
  "type": "project",
  "config": {
	"preferred-install": "dist",
	"optimize-autoloader": true,
	"classmap-authoritative": true
  },
  "authors": [
	{
	  "name": "Leo Ning",
	  "email": "windywany@gmail.com"
	}
  ],
  "extra": {
	"branch-alias": {
	  "dev-master": "1.0-dev"
	}
  },
  "support": {
